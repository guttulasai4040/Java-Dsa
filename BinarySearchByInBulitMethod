import java.util.Arrays;

class BinarySearch{
    public static void main(String[] args) {
        int[] arr = {1,2,3,4,5,6,7,8,9};
        int num = 7;
        int index = Arrays.binarySearch(arr, num);
        
        if(index >= 0) {
            System.out.println("Key number in the " + index); 
        } else {
            System.out.println("key is not in the ArrayList ");
        }
    }
}
